<p>
  <strong>First name:</strong>
  <%= @owner.first_name %>
</p>

<p>
  <strong>Last name:</strong>
  <%= @owner.last_name %>
</p>

<p>
  <strong>Phone:</strong>
  <%= @owner.phone %>
</p>

<p>
  <strong>Evac Waiver:</strong>
  <%= @owner.evac_waiver %>
</p>

<div class="row">
  <% @dogs.each do |dog| %>
    <div class="col-sm-4 col-xs-3 col-med-4">
      <div class="panel-heading">
        <h3 class="panel-title"> <%= dog.name %></h3>
      </div>
      <div class="panel-body">
        <%= image_tag dog.avatar.url(:medium), class: 'img-rounded img-responsive' %>
      </div>

    </div>
  <% end %>
</div>

<br>

<%= link_to 'Edit', edit_owner_path(@owner), class: "btn btn-primary" %>
<%= link_to 'Back', owners_path, class: "btn btn-primary" %>